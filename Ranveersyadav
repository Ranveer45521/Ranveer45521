    <script>
        // Mobile menu toggle
        function toggleMobileMenu() {
            const mobileMenu = document.getElementById('mobileMenu');
            mobileMenu.classList.toggle('hidden');
        }

        // Standard calculator
        function calculateStandardCharges() {
            const buyPrice = parseFloat(document.getElementById('buyPrice').value);
            const sellPrice = parseFloat(document.getElementById('sellPrice').value);
            const quantity = parseInt(document.getElementById('quantity').value);
            const broker = document.getElementById('broker').value;
            
            // Sample calculation
            const turnover = (buyPrice + sellPrice) * quantity;
            const brokerage = Math.min(turnover * 0.0003, 40); // 0.03% or â‚¹40 max
            const stt = turnover * 0.001; // 0.1%
            const exchangeCharges = turnover * 0.0000325; // 0.00325%
            const gst = (brokerage + exchangeCharges) * 0.18;
            const totalCharges = brokerage + stt + exchangeCharges + gst;
            const netPnL = (sellPrice - buyPrice) * quantity - totalCharges;
            
            document.getElementById('standardResults').innerHTML = `
                <div class="text-center">
                    <div class="text-3xl font-bold text-green-600 mb-2">â‚¹${totalCharges.toFixed(2)}</div>
                    <p class="text-gray-600 mb-4">Total Brokerage Charges</p>
                    <div class="grid grid-cols-1 md:grid-cols-3 gap-4 text-sm">
                        <div class="bg-gray-50 p-4 rounded-lg">
                            <div class="font-semibold text-gray-900">Net P&L</div>
                            <div class="text-${netPnL >= 0 ? 'green' : 'red'}-600 text-lg font-bold">â‚¹${netPnL.toFixed(2)}</div>
                        </div>
                        <div class="bg-gray-50 p-4 rounded-lg">
                            <div class="font-semibold text-gray-900">Effective Rate</div>
                            <div class="text-blue-600 text-lg font-bold">${((totalCharges/turnover)*100).toFixed(3)}%</div>
                        </div>
                        <div class="bg-gray-50 p-4 rounded-lg">
                            <div class="font-semibold text-gray-900">Turnover</div>
                            <div class="text-purple-600 text-lg font-bold">â‚¹${turnover.toFixed(2)}</div>
                        </div>
                    </div>
                    <div class="mt-4 text-left">
                        <h4 class="font-semibold mb-2">Breakdown:</h4>
                        <div class="space-y-1 text-sm">
                            <div class="flex justify-between"><span>Brokerage:</span><span>â‚¹${brokerage.toFixed(2)}</span></div>
                            <div class="flex justify-between"><span>STT:</span><span>â‚¹${stt.toFixed(2)}</span></div>
                            <div class="flex justify-between"><span>Exchange Charges:</span><span>â‚¹${exchangeCharges.toFixed(2)}</span></div>
                            <div class="flex justify-between"><span>GST:</span><span>â‚¹${gst.toFixed(2)}</span></div>
                        </div>
                    </div>
                </div>
            `;
        }

        // Custom calculator
        function calculateCustomCharges() {
            const buyPrice = parseFloat(document.getElementById('customBuyPrice').value);
            const sellPrice = parseFloat(document.getElementById('customSellPrice').value);
            const quantity = parseInt(document.getElementById('customQuantity').value);
            const brokerageRate = parseFloat(document.getElementById('brokerageRate').value) / 100;
            const sttRate = parseFloat(document.getElementById('sttRate').value) / 100;
            const exchangeRate = parseFloat(document.getElementById('exchangeCharges').value) / 100;
            
            const turnover = (buyPrice + sellPrice) * quantity;
            const brokerage = turnover * brokerageRate;
            const stt = turnover * sttRate;
            const exchangeCharges = turnover * exchangeRate;
            const gst = (brokerage + exchangeCharges) * 0.18;
            const totalCharges = brokerage + stt + exchangeCharges + gst;
            const netPnL = (sellPrice - buyPrice) * quantity - totalCharges;
            
            document.getElementById('customResults').innerHTML = `
                <div class="text-center">
                    <div class="text-3xl font-bold text-purple-600 mb-2">â‚¹${totalCharges.toFixed(2)}</div>
                    <p class="text-gray-600 mb-4">Total Custom Charges</p>
                    <div class="grid grid-cols-1 md:grid-cols-3 gap-4 text-sm">
                        <div class="bg-gray-50 p-4 rounded-lg">
                            <div class="font-semibold text-gray-900">Net P&L</div>
                            <div class="text-${netPnL >= 0 ? 'green' : 'red'}-600 text-lg font-bold">â‚¹${netPnL.toFixed(2)}</div>
                        </div>
                        <div class="bg-gray-50 p-4 rounded-lg">
                            <div class="font-semibold text-gray-900">Effective Rate</div>
                            <div class="text-blue-600 text-lg font-bold">${((totalCharges/turnover)*100).toFixed(3)}%</div>
                        </div>
                        <div class="bg-gray-50 p-4 rounded-lg">
                            <div class="font-semibold text-gray-900">Turnover</div>
                            <div class="text-purple-600 text-lg font-bold">â‚¹${turnover.toFixed(2)}</div>
                        </div>
                    </div>
                    <div class="mt-4 text-left">
                        <h4 class="font-semibold mb-2">Custom Breakdown:</h4>
                        <div class="space-y-1 text-sm">
                            <div class="flex justify-between"><span>Brokerage (${(brokerageRate*100).toFixed(2)}%):</span><span>â‚¹${brokerage.toFixed(2)}</span></div>
                            <div class="flex justify-between"><span>STT (${(sttRate*100).toFixed(2)}%):</span><span>â‚¹${stt.toFixed(2)}</span></div>
                            <div class="flex justify-between"><span>Exchange (${(exchangeRate*100).toFixed(5)}%):</span><span>â‚¹${exchangeCharges.toFixed(2)}</span></div>
                            <div class="flex justify-between"><span>GST (18%):</span><span>â‚¹${gst.toFixed(2)}</span></div>
                        </div>
                    </div>
                </div>
            `;
        }

        // Contact form submission
        function submitContact() {
            const name = document.getElementById('contactName').value;
            const email = document.getElementById('contactEmail').value;
            const subject = document.getElementById('contactSubject').value;
            const message = document.getElementById('contactMessage').value;
            
            // In a real application, you would send this data to your server
            alert(`Thank you ${name}! Your message has been received. We'll get back to you soon.`);
            
            // Reset form
            document.getElementById('contactForm').reset();
        }

        // Smooth scrolling for navigation links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });
